
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id String @id @default(cuid())
  name String @db.VarChar(100)
  email String @unique 
  password String 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}